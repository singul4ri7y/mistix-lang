cls Data {
    construct[name, passion, age] {
        sel -> name    = name
        sel -> passion = passion
    }

    query[] {
        print('Hello! I'm ' + sel -> name + ' and I like to do ' + sel -> passion +
            ' a lot!')
    }
}

var data = Data('SD Asif Hossein', 'coding', 21)

data -> query()

@@ Expected Output: 
@ Hello! I'm SD Asif Hossein and I like to do coding a lot!
